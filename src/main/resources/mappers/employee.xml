<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.study.springboot202010kkr.repository.EmployeeRepository">
    <insert id="saveEmployee" parameterType="com.study.springboot202010kkr.web.dto.EmployeeDto"
    useGeneratedKeys="true" keyProperty="empId">
        insert into emp_mst
        values (0, #{empName}, #{empAge}, #{empPhone})
    </insert>
</mapper>


